<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/background_foret"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_admin_session"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kagura.project.com.a8.admin.AdminSession">


    <Button
        android:id="@+id/buttonResultAssociation"
        android:text="@string/results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="results"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="153dp"
        android:layout_marginStart="153dp" />

    <Button
        android:id="@+id/buttonResultMemory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/buttonResultAssociation"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="results"
        android:layout_marginRight="246dp"
        android:layout_marginEnd="246dp" />
</RelativeLayout>
